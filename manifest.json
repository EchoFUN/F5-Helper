{
   "manifest_version": 2,
   "name": "F5-Helper",
   "version": "0.0.1",
   "description": "You know it",
   "browser_action": {
      "default_icon": "icon.png"
   },
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "js": ["app.js"],
         "css": ["app.css"],
         "run_at": "document_start",
         "all_frames": false
      }
   ],
   "background": {
      "scripts": ["background.js"]
   }
}
